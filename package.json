{
  "name": "idxr-demo",
  "version": "1.0.0",
  "description": "Identity Cross-Resolution System Demo Implementation",
  "main": "backend/server.js",
  "scripts": {
    "start": "node scripts/start-server.js",
    "start-direct": "node backend/server.js",
    "start-force": "node scripts/start-server.js --force-start",
    "dev": "nodemon backend/server.js",
    "frontend": "cd frontend && npm start",
    "test": "jest",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "seed": "node scripts/seedDatabase.js",
    "status": "node -e \"const fs=require('fs'); const path=require('path'); const pidFile=path.join(__dirname,'server.pid'); if(fs.existsSync(pidFile)){const pid=fs.readFileSync(pidFile,'utf8').trim(); console.log('Server PID:',pid); try{process.kill(pid,0); console.log('Status: RUNNING')}catch{console.log('Status: STOPPED (stale PID)')}}else{console.log('Status: STOPPED')}\"",
    "health-check": "node scripts/check-consistency.js"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "helmet": "^7.0.0",
    "joi": "^17.10.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "uuid-v4": "^0.1.0",
    "winston": "^3.11.0",
    "ws": "^8.18.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "author": "Colorado OIT",
  "license": "MIT"
}
